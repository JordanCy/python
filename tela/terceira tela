from PyQt5 import uic, QtWidgets




def chama_terceira_tela():
    fotocata.show()
    

app=QtWidgets.QApplication([])
cadastronew=uic.loadUi('cadastronew.ui')
fotocata=uic.loadUi('fotocata.ui')
cadastronew.incresverse.clicked.connect(chama_terceira_tela)
    
cadastronew.show()
    
app.exec()